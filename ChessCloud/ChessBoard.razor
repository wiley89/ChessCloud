@using ChessCloud.Data
@code {

    static string fen = "rnbqkbnr/pppppppp/8/8/8/8/PPPPPPPP/RNBQKBNR w KQkq - 0 1";

    static ChessGame game = new ChessGame();
    string[,] board = game.generateBoard(fen);



    Piece[,] pieceBoard = game.generatePieceBoard(fen);


}

<h3>ChessBoard: @pieceBoard[0,0].Color</h3>

<input />
<button>New Game</button>

@for (int i = 0; i < 8; i++)
{
    <div class="row">
        @for (int j = 0; j < 8; j++)
        {

        <div class="cell">
            @if (board[i, j] != "em")
            {
                //Piece piece = pieceBoard[i, j];
                <div class="piece  @pieceBoard[i,j].Type @pieceBoard[i,j].Color">

                </div>
            }
        </div>
        }
    </div>
}

